@model List<ProductListComponentViewModel>

<table class="table  table-dark table-striped">
    <thead>
        <tr>
            <th>İsim</th>
            <th>Açıklama</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Name</td>
                <td>@product.Description</td>
            </tr>
        }
    </tbody>
</table>